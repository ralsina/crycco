{% extends "page.j2" %}
{% block body %}
<div id="title">
    <h1>
        {{title}}
    </h1>
</div>
{% for section in sections %}
<div id="section-{{loop.index}}">
    {% if section.doc != "" %}
    <div class="doc">
        {{ section.docs_html}}
    </div>
    {% endif %}
    {% if section.code != "" %}
    <div class="code">
        {{ section.code_html}}
    </div>
    {% endif %}
</div>
{% endfor %}
{% endblock %}